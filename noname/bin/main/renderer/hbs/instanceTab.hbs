<div class="noname-tab">
  <h3>Connection Details</h3>
  <table class="table table-bordered">
    <tr>
      <th>Dbtype</th>
      <td>{{type}}</td>
    </tr>
    <tr>
      <th>Username</th>
      <td>{{username}}</td>
    </tr>
    <tr>
      <th>Password</th>
      <td>{{password}}</td>
    </tr>
    <tr>
      <th>Host</th>
      <td>{{host}}</td>
    </tr>
    <tr>
      <th>Port</th>
      <td>{{port}}</td>
    </tr>
    <tr>
      <th>Database</th>
      <td>{{database}}</td>
    </tr>
    <tr>
      <th>Connection String</th>
      <td><code>{{connectionString}}</code></td>
    </tr>
  </table>
</div>
